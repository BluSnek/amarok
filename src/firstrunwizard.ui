                    <enum>Vertical</enum>
                </property>
                <property name="sizeType">
                    <enum>Expanding</enum>
                </property>
                <property name="sizeHint">
                    <size>
                        <width>20</width>
                        <height>141</height>
                    </size>
                </property>
            </spacer>
            <widget class="QLabel" row="0" column="1">
                <property name="name">
