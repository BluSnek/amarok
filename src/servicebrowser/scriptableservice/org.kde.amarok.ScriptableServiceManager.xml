<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.amarok.ScriptableServiceManager">
    <method name="initService">
      <arg type="b" direction="out"/>
      <arg name="name" type="s" direction="in"/>
      <arg name="levels" type="i" direction="in"/>
      <arg name="shortDescription" type="s" direction="in"/>
      <arg name="rootHtml" type="s" direction="in"/>
      <arg name="showSearchBar" type="b" direction="in"/>
    </method>
    <method name="insertItem">
      <arg type="i" direction="out"/>
      <arg name="serviceName" type="s" direction="in"/>
      <arg name="level" type="i" direction="in"/>
      <arg name="parentId" type="i" direction="in"/>
      <arg name="name" type="s" direction="in"/>
      <arg name="infoHtml" type="s" direction="in"/>
      <arg name="callbackData" type="s" direction="in"/>
      <arg name="playableUrl" type="s" direction="in"/>
    </method>
    <method name="donePopulating">
      <arg name="serviceName" type="s" direction="in"/>
      <arg name="parentId" type="i" direction="in"/>
    </method>
  </interface>
</node>
